{
  "name": "trello-competitor",
  "scripts": {
    "dev": "nodemon -w src",
    "lint": "eslint src",
    "start": "node --experimental-specifier-resolution node -r dotenv/config src/server.js",
    "test": "cross-env DEBUG=rs:* jest --testMatch \"<rootDir>/test/e2e/test/*.test.js\" --noStackTrace --runInBand",
    "test:auth": "cross-env DEBUG=rs:* TEST_MODE=auth jest --noStackTrace"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./test/setup.js"
    ]
  },
  "dependencies": {
    "@koa/router": "^10.1.1",
    "dotenv": "10.0.0",
    "knex": "^0.95.14",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "objection": "^3.0.0",
    "pino": "^7.5.1",
    "pino-pretty": "^7.2.0",
    "sqlite3": "^5.0.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@shcherbin/eslint-config": "^0.2.0",
    "chai": "4.3.4",
    "cross-env": "^7.0.3",
    "debug": "4.3.3",
    "dirty-chai": "2.0.1",
    "eslint": "^8.3.0",
    "jest": "27.4.0",
    "nodemon": "2.0.15",
    "supertest": "6.1.6",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": "16.x"
  },
  "type": "module",
  "private": true
}
