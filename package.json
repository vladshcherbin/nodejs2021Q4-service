{
  "name": "trello-competitor",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon -w dist",
    "lint": "eslint src --ext .ts",
    "start": "node --experimental-specifier-resolution node -r dotenv/config dist/server.js",
    "test": "cross-env DEBUG=rs:* jest --testMatch \"<rootDir>/test/e2e/test/*.test.js\" --noStackTrace --runInBand"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./test/setup.js"
    ]
  },
  "dependencies": {
    "@koa/router": "^10.1.1",
    "dotenv": "10.0.0",
    "is-plain-obj": "^4.0.0",
    "knex": "^0.21.21",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "objection": "^2.2.18",
    "pino": "^7.5.1",
    "pino-pretty": "^7.3.0",
    "sqlite3": "^5.0.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@shcherbin/eslint-config": "^0.3.1",
    "@types/koa": "^2.13.4",
    "@types/koa-bodyparser": "^4.3.4",
    "@types/koa__router": "^8.0.11",
    "chai": "4.3.4",
    "cross-env": "^7.0.3",
    "debug": "4.3.3",
    "dirty-chai": "2.0.1",
    "eslint": "^8.5.0",
    "jest": "27.4.5",
    "nodemon": "2.0.15",
    "supertest": "6.1.6",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": "16.x"
  },
  "type": "module",
  "private": true
}
