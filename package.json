{
  "name": "trello-competitor",
  "scripts": {
    "build": "tsc",
    "db:latest": "yarn knex migrate:latest",
    "db:rollback": "yarn knex migrate:rollback",
    "dev": "nodemon -x node --experimental-loader ts-node/esm --experimental-specifier-resolution node -r dotenv/config src/server.ts -e ts -w src",
    "lint": "eslint src --ext .ts",
    "start": "node --experimental-specifier-resolution node -r dotenv/config dist/server.js",
    "test": "cross-env DEBUG=rs:* jest --testMatch \"<rootDir>/test/e2e/test/*.test.js\" --noStackTrace --runInBand"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./test/setup.js"
    ]
  },
  "dependencies": {
    "@koa/router": "^10.1.1",
    "colorette": "^2.0.16",
    "dotenv": "14.2.0",
    "is-plain-obj": "^4.0.0",
    "knex": "^1.0.1",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "objection": "^3.0.1",
    "pg": "^8.7.1",
    "pino": "^7.6.4",
    "pino-pretty": "^7.5.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@shcherbin/eslint-config": "^0.3.1",
    "@swc/core": "^1.2.133",
    "@types/koa": "^2.13.4",
    "@types/koa-bodyparser": "^4.3.5",
    "@types/koa__router": "^8.0.11",
    "chai": "4.3.4",
    "cross-env": "^7.0.3",
    "debug": "4.3.3",
    "dirty-chai": "2.0.1",
    "eslint": "^8.7.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "jest": "27.4.7",
    "nodemon": "2.0.15",
    "supertest": "6.2.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": "16.x"
  },
  "type": "module",
  "private": true
}
