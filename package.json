{
  "name": "trello-competitor",
  "scripts": {
    "build": "tsc",
    "db:latest": "yarn knex migrate:latest",
    "db:rollback": "yarn knex migrate:rollback",
    "dev": "nodemon -x node --experimental-loader ts-node/esm --experimental-specifier-resolution node src/server.ts -e ts -w src",
    "lint": "eslint src --ext .ts",
    "start": "node --experimental-specifier-resolution node dist/server.js",
    "test": "cross-env DEBUG=rs:* jest --testMatch \"<rootDir>/test/e2e/test/*.test.js\" --noStackTrace --runInBand",
    "test:auth": "cross-env DEBUG=rs:* TEST_MODE=auth jest --noStackTrace",
    "test:load": "artillery run artillery.yaml"
  },
  "dependencies": {
    "@nestjs/common": "^8.2.6",
    "@nestjs/config": "^1.1.6",
    "@nestjs/core": "^8.2.6",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/mapped-types": "^1.0.1",
    "@nestjs/passport": "^8.1.0",
    "@nestjs/platform-express": "^8.2.6",
    "@nestjs/platform-fastify": "^8.2.6",
    "@webundsoehne/nest-fastify-file-upload": "^1.0.0",
    "@willsoto/nestjs-objection": "^7.0.0",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "fastify-multer": "^2.0.2",
    "fastify-multipart": "^5.3.0",
    "knex": "^1.0.2",
    "nestjs-pino": "^2.5.0",
    "objection": "^3.0.1",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "pg": "^8.7.3",
    "pino-http": "^6.6.0",
    "pino-pretty": "^7.5.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.0",
    "@shcherbin/eslint-config": "^0.3.1",
    "@types/bcrypt": "^5.0.0",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.11.22",
    "@types/passport-jwt": "^3.0.6",
    "artillery": "^2.0.0-11",
    "chai": "4.3.4",
    "cross-env": "^7.0.3",
    "debug": "4.3.3",
    "dirty-chai": "2.0.1",
    "eslint": "^8.7.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "jest": "27.4.7",
    "nodemon": "2.0.15",
    "supertest": "6.2.2",
    "ts-node": "^10.4.0",
    "typescript": "4.5.2"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./test/setup.js"
    ]
  },
  "engines": {
    "node": "16.x"
  },
  "type": "module",
  "private": true
}
